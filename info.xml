<?xml version="1.0"?>
<extension key="sumfields-addon" type="module">
    <file>sumfields_addon</file>
    <name>Summary fields extra</name>
    <description>Additonal summary fields for the net.ourpowerbase.sumfields extension</description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Sandor Semsey</author>
        <email>sandor@es-progress.hu</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/sumfields-addon</url>
        <url desc="Documentation">https://github.com/reflexive-communications/sumfields-addon</url>
        <url desc="Support">https://github.com/reflexive-communications/sumfields-addon/issues</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2021-07-22</releaseDate>
    <version>0.3.0</version>
    <develStage>alpha</develStage>
    <compatibility>
        <ver>5.0</ver>
    </compatibility>
    <requires>
        <ext>net.ourpowerbase.sumfields</ext>
    </requires>
    <classloader>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <civix>
        <namespace>CRM/SumfieldsAddon</namespace>
    </civix>
</extension>
